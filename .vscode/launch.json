    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: main",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main_crossformer.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {"PYTHONPATH": "d:/ProgramData/miniconda3/envs/e1/bin/"},
            "python": "d:/ProgramData/miniconda3/envs/e1/python.exe"
        },
        {
            "name": "Python Debugger: current",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${fileDirname}",
            "env": {"PYTHONPATH": "d:/ProgramData/miniconda3/envs/e1/bin/"}
        }, 
        {
            "name": "Python Debugger: current s1",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${fileDirname}",
            "env": {"PYTHONPATH": "d:/ProgramData/miniconda3/envs/s1/bin/"}
        }        
    ]
}